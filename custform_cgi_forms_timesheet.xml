        <visible>T</visible>
      </button>
      <button>
        <id>REJECTALLPENDING</id>
        <label>Reject All Pending</label>
        <visible>T</visible>
      </button>
      <button>
        <id>REJECTALLPENDINGWITHNOTE</id>
